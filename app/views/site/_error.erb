

<div class="container">
    <pre class="language-markup">
        Resultados: <%= params.to_json.html_safe %><br>
        SignIn: <%= @signIn.to_json.html_safe %><br>
        Mensagens: <%= @mensagens.to_json.html_safe %><br>
        Response: <%= @response.to_json.html_safe %><br>
        Relatorio: <%=@relatorio.to_json.html_safe %><br>

        <% flash.each do |name, msg| %>
            <%= content_tag :div, "Nome: #{name} | Conteudo:#{msg}", :id => "flash_#{name}" if msg.is_a?(String) %> <br><br>
        <% end %>

        Alert: <%= flash[:alert].to_json.html_safe %><br>
        Notices: <%= flash[:notice] %><br>
        Dados: <%= flash[:dados] %><br>
    </pre>
</div>